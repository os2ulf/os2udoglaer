        "drupal/editor_advanced_link": "^2.2",
        "drupal/entity_clone": "^2.0@beta",
        "drupal/entity_diff_ui": "^2.0@alpha",
        "drupal/entity_usage": "^2.0-beta3",
        "drupal/facets": "^2.0",
        "drupal/field_group": "^3.0",
        "drupal/focal_point": "^2.0",
        "drupal/gin": "^3.0",
        "drupal/gin_toolbar": "^1.0@RC",
        "drupal/health_check": "^3.0",
        "drupal/hreflang": "^1.0",
        "drupal/imageapi_optimize": "^4.0",
        "drupal/inline_block_title_automatic": "^1.0",
        "drupal/jquery_once": "^1.1",
        "drupal/layout_builder_at": "^2.0",
        "drupal/layout_builder_modal": "^1.0",
        "drupal/layout_builder_restrictions": "^2.0",
        "drupal/layoutbuilder_search_api": "^1.0",
        "drupal/linkit": "^6.0",
        "drupal/maxlength": "^3.0@beta",
        "drupal/menu_item_fields": "^1.10",
        "drupal/menu_per_role": "^1.7",
        "drupal/metatag": "^1.0",
        "drupal/noreferrer": "^1.0",
        "drupal/paragraphs": "^1.0",
        "drupal/paragraphs_asymmetric_translation_widgets": "^1.0",
        "drupal/pathauto": "^1.0",
        "drupal/pathologic": "^2.0@alpha",
        "drupal/r4032login": "^2.2",
        "drupal/rdf": "^2.0",
        "drupal/redirect": "^1.0",
        "drupal/redirect_after_login": "^3.0",
        "drupal/redis": "^1.5",
        "drupal/restui": "^1.21",
        "drupal/role_delegation": "^1.1",
        "drupal/scheduled_transitions": "^2.0",
        "drupal/search_api": "^1.0",
        "drupal/search_api_exclude_entity": "^1.0",
        "drupal/search_api_page": "^1.0@beta",
        "drupal/search_api_solr": "^4.3",
        "drupal/sendgrid_integration": "^2.2",
        "drupal/sharedemail": "^3.0",
        "drupal/simple_access_log": "^2.0",
        "drupal/simple_sitemap": "^4.1",
        "drupal/smtp": "^1.2",
        "drupal/symfony_mailer_lite": "^2.0",
        "drupal/transform_api": "1.1.0-beta3",
        "drupal/transform_api_facets": "^1.0@alpha",
        "drupal/ultimate_cron": "^2.0@alpha",
        "drupal/view_profiles_perms": "^2.1",
        "drupal/view_unpublished": "^1.0",
        "drupal/views_base_url": "^2.1@beta",
        "drupal/views_bulk_operations": "^4.3",
        "drupal/views_cm_current_state": "^2.1",
        "drupal/views_contextual_filters_or": "^1.4",
        "drupal/views_data_export": "^1.4",
        "drupal/views_simple_math_field": "^3.0",
        "drupal/webform": "^6.2",
        "drupal/webform_rest": "^4.1",
        "drupal/webp": "^1.0@RC",
        "drupal/workbench": "^1.4",
        "drupal/workbench_reviewer": "^3.0@beta",
        "drupal/workflow_buttons": "^1.0@beta",
        "drupal/xls_serialization": "^1.3",
        "drush/drush": "^12",
        "novicell/dawa": "^1.0",
        "novicell/dynamic_key_value": "^1.0",
        "novicell/image_style_generator": "^1.0",
        "novicell/media_icons": "^1.0",
        "novicell/premium_core": "^1.0",
        "novicell/premium_headless": "^1.0",
        "novicell/premium_headless_theme": "^1.0",
        "novicell/premium_profile": "^1.0",
        "novicell/premium_search": "^1.0",
        "novicell/premium_tools": "^1.0",
        "novicell/status_feed": "^1.0",
        "novicell/styles": "^1.0",
        "oomphinc/composer-installers-extender": "^2.0",
        "platformsh/config-reader": "^2.4",
        "vlucas/phpdotenv": "^5.3",
        "wikimedia/composer-merge-plugin": "^2.0"
    },
    "require-dev": {
        "drupal/devel": "^5.0",
        "drupal/upgrade_status": "^4.0",
        "kint-php/kint": "^5.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "platform": {
            "php": "8.3"
        },
        "optimize-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "drupal/core-vendor-hardening": true,
            "composer/installers": true,
            "wikimedia/composer-merge-plugin": true,
            "oomphinc/composer-installers-extender": true
        }
    },
    "extra": {
        "drupal-scaffold": {
            "allowed-packages": [
                "drupal/core"
            ],
            "locations": {
                "web-root": "webroot/"
            },
            "file-mapping": {
                "[web-root]/robots.txt": {
                    "append": "assets/robots-additions.txt"
                },
                "[web-root]/sites/development.services.yml": {
                    "mode": "replace",
                    "path": "assets/development.services.yml",
                    "overwrite": false
                }
            }
        },
        "installer-paths": {
            "webroot/core": [
                "type:drupal-core"
            ],
            "webroot/libraries/{$name}": [
                "type:drupal-library",
                "type:npm-asset",
                "type:bower-asset"
            ],
            "webroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "webroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "webroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "webroot/drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "webroot/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "webroot/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "installer-types": [
            "npm-asset",
            "bower-asset"
        ],
        "merge-plugin": {
            "include": [
                "webroot/modules/contrib/webform/composer.libraries.json"
            ]
        },
        "patches": {
            "drupal/core": {
                "(#2999491) Allow creation of reuseable inline blocks": "https://www.drupal.org/files/issues/2024-07-17/drupal-2999491-162.patch",
                "Configure limit method on titles and link fields": "patches/configure_limit_method_on_titles_and_link_fields.patch"
            },
            "drupal/autosave_form": {
                "(#3355495) PHP 8.2 deprecation issue with AutosaveFormBuilder": "https://www.drupal.org/files/issues/2023-04-20/autosave_form-dynamic-property-deprecation.patch",
                "(#3487459) Deprecation error on user edit form": "https://www.drupal.org/files/issues/2024-11-18/3487459-4.patch"
            },
            "drupal/conditional_fields": {
                "(#2988937) An invalid form control with name='body[0][value]' is not focusable. with ckeditor enabled": "https://www.drupal.org/files/issues/2023-04-25/dependency-using-body-as-target-2988937-12.patch"
            },
            "drupal/content_moderation_notifications": {
                "(#3419565) Allow a notification to bypass the content access check": "https://www.drupal.org/files/issues/2024-02-06/content_moderation_notifications-bypass_access_check-3419565-2.patch",
                "(#2965600) Add content type option": "https://www.drupal.org/files/issues/2025-01-24/2965600-content-type_20.patch",
                "Enable token support for ad-hoc email addresses": "patches/content_moderation_notifications_support_adhoc_token.patch",
                "Turn BCC into To": "patches/content_moderation_notifications_bcc_into_to.patch"
            },
            "drupal/diff": {
                "(#3165141) Set the default filter to 'Strip Tags'": "https://www.drupal.org/files/issues/2024-09-16/3165141-21.patch"
            },
            "drupal/domain_entity" : {
                "(#3405907) Make module compatible with PHP 8.2": "https://www.drupal.org/files/issues/2023-12-04/3405907-1.patch"
            },
            "drupal/domain_login_restrict" : {
                "(#3336184) The configuration is not exported": "https://www.drupal.org/files/issues/2024-02-12/domain_login_restrict-3336184-9.patch"
            },
            "drupal/paragraphs": {
                "(#2901390) Integrity constraint violation: 1048 Column 'langcode' cannot be null": "https://www.drupal.org/files/issues/2023-07-20/2901390-9-109.patch"
            },
            "drupal/redis": {
                "(#3337750) DivisionByZeroError - Problem to access Status Page on PHP 8.1.14": "https://www.drupal.org/files/issues/2024-09-05/redis_status_page_divizion_by_zero_error_php8_1_14_3337750-13.patch"
            },
            "drupal/scheduled_transitions": {
                "(#3264674) Field groups are added to the scheduled transitions add form": "https://www.drupal.org/files/issues/2022-02-16/scheduled_transitions-field_groups_add_form-3264674-2.patch"
            },
            "drupal/transform_api": {
                "Fix access check for non-bundled entity types": "patches/transform_api_non_bundled_entity_access_fix.patch",
                "Add getter for field transform": "patches/transform_api_add_field_transform_getter.patch",
                "Fix views parameters": "https://git.drupalcode.org/project/transform_api/-/commit/190b4a236137133ecb4710a5067b8510c9cf6661.diff",
                "Fix views caching": "https://git.drupalcode.org/project/transform_api/-/commit/83e2d5b672ade36a9814681063ef498ef9dd1e68.diff",
                "Fix views mini pager": "https://git.drupalcode.org/project/transform_api/-/commit/85f532fcf48f0083436d0c4716b328d9c41d66ce.diff",
                "#(3458752) Empty region transform results in error": "https://www.drupal.org/files/issues/2024-07-03/transform_api_empty_region.patch",
                "Allow universal search api entity result transform": "https://git.drupalcode.org/project/transform_api/-/commit/802912d2974ab10f4b6fbc7c0e140333d48f0227.diff",
                "Add #view to view transformation": "https://git.drupalcode.org/project/transform_api/-/commit/4332f4e5014b196e9a42b047ec8f0cb3bd0b5ba6.diff",
                "Fix better_exposed_filters label": "https://git.drupalcode.org/project/transform_api/-/commit/ce1b8214ef523ca859509957e5cd7032e452df49.diff",
                "Fixed view results route for search_api usage": "https://git.drupalcode.org/project/transform_api/-/commit/11ec5670060f96bcafb815e0ee5691480530f1e6.diff",
                "Added label to view transform": "https://git.drupalcode.org/project/transform_api/-/commit/e4721c4d8d73749ddbbd71a71ee4959c66318106.diff",
                "Fixed sorting of view exposed filters": "https://git.drupalcode.org/project/transform_api/-/commit/c97e8259a5eaffedb55f0628ac4f3614067484c3.diff",
                "Fixed error of exposed filters": "https://git.drupalcode.org/project/transform_api/-/commit/c38b7060b9e1333469a877fe393303ac148eaf9f.diff",
                "Fixed error message": "https://git.drupalcode.org/project/transform_api/-/commit/12d92a3e541c0c144b7d89aacdff879f761a6a68.diff",
                "Fixed view results bug": "https://git.drupalcode.org/project/transform_api/-/commit/81cb0b94fdbd1c20317748d3bcce563306268b77.diff"
            },
            "drupal/webform": {
                "(#3435903) Fix incompatibility with devel ^5.2": "https://www.drupal.org/files/issues/2024-08-01/fix_incompatibility_with_devel_5.2.patch"
            },
            "drupal/views_bulk_operations": {
                "(#3422715) Missing checkboxes in 4.2.6 when using Views Data Export": "https://git.drupalcode.org/project/views_bulk_operations/-/merge_requests/79.diff",
                "(#3210957) VBO is skipping translated entities during batch process": "https://git.drupalcode.org/project/views_bulk_operations/-/merge_requests/66.diff"
            },
            "drupal/workbench": {
                "Apply patch from branch 3484630-mismatched-views-area": "https://git.drupalcode.org/issue/workbench-3484630/-/commit/41ff2be540e760aec54e06cd35c5e5a75e21f502.diff"
            },
            "drupal/workflow_buttons": {
                "(#3451409) Automated Drupal 11 compatibility fixes for workflow_buttons": "https://www.drupal.org/files/issues/2024-11-25/workflow_buttons_3451409_git_theme.patch"
            },
            "novicell/dawa": {
                "Require full address, or none": "patches/dawa_require_full_address_or_none.patch"
            }
        }
    }
}
