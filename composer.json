        "drupal/autosave_form": "^1.0",
        "drupal/better_exposed_filters": "^6.0",
        "drupal/better_permissions_page": "^1.0",
        "drupal/ckeditor5_paste_filter": "^1.0",
        "drupal/components": "^3.0@beta",
        "drupal/conditional_fields": "^4.0@alpha",
        "drupal/config_ignore": "^3.3",
        "drupal/config_split": "^1.7.0",
        "drupal/content_moderation_bypass": "^1.0@alpha",
        "drupal/content_moderation_notifications": "^3.6",
        "drupal/core-composer-scaffold": "^10.3",
        "drupal/core-project-message": "^10.3",
        "drupal/core-recommended": "^10.3",
        "drupal/core-vendor-hardening": "^10.3",
        "drupal/date_popup": "^2.0",
        "drupal/diff": "^1.7",
        "drupal/domain": "^2.0@beta",
        "drupal/domain_access_search_api": "^1.0",
        "drupal/domain_entity": "^1.0@RC",
        "drupal/domain_login_restrict": "^1.2",
        "drupal/domain_menus": "^3.6",
